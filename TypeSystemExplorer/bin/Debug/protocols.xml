        <NativeType Name="OrderBy" ImplementingType="string"/>
        <NativeType Name="GroupBy" ImplementingType="string"/>
        <NativeType Name="Tag" ImplementingType="string"/>
        <NativeType Name="UniqueKey" ImplementingType="string"/>            <!-- for "InsertIfMissing" to identify a non-PK UK -->
        <NativeType Name="UniqueKeyValue" ImplementingType="string"/>       <!-- for "GetID" based on a single field UK value -->
