<?xml version="1.0" encoding="utf-8" ?>
<SemanticTypes>
  <Collection>
    <SemanticTypeDecl OfType="Noun">
      <!-- The ST has a Name attribute because it's Struct defines it. -->
      <AttributeValues>
        <Attribute Name="Name" Value="Noun"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="Noun">
      <Attributes>
        <!-- here the attribute Name's value is the implementing field name -->
        <!-- For example, in C#, resulting form is "string Name;" -->
        <NativeType Name="Name" ImplementingType="string"/>
      </Attributes>
    </SemanticTypeStruct>
    
    <!-- HIGHER LEVEL SEMANTIC TYPES -->
    
    <!-- declarations -->

    <SemanticTypeDecl OfType="PrimaryKey">
      <AttributeValues>
        <Attribute Name="Name" Value="PrimaryKey"/>
        <Attribute Name="Value"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="URL"/>
        <Attribute Name="Value"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="ImageFilename"/>
        <Attribute Name="Value"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="Text"/>
        <Attribute Name="Value"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="Keywords"/>
        <Attribute Name="Value"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="Title"/>
        <Attribute Name="Value"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="Explanation"/>
        <Attribute Name="Value"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <!-- structs -->

    <SemanticTypeStruct DeclType="PrimaryKey">
      <Attributes>
        <NativeType Name="Value" ImplementingType="long?"/>
      </Attributes>
    </SemanticTypeStruct>

    <SemanticTypeStruct DeclType="URL">
      <Attributes>
        <NativeType Name="Value" ImplementingType="string"/>
      </Attributes>
    </SemanticTypeStruct>

    <SemanticTypeStruct DeclType="ImageFilename">
      <Attributes>
        <NativeType Name="Filename" ImplementingType="string"/>
      </Attributes>
    </SemanticTypeStruct>

    <SemanticTypeStruct DeclType="Text">
      <Attributes>
        <NativeType Name="Value" ImplementingType="string"/>
      </Attributes>
    </SemanticTypeStruct>

    <SemanticTypeStruct DeclType="Keywords">
      <Attributes>
        <SemanticElement Name="Text"/>
      </Attributes>
    </SemanticTypeStruct>

    <SemanticTypeStruct DeclType="Title">
      <Attributes>
        <SemanticElement Name="Text"/>
      </Attributes>
    </SemanticTypeStruct>

    <SemanticTypeStruct DeclType="Explanation">
      <Attributes>
        <SemanticElement Name="Text"/>
      </Attributes>
    </SemanticTypeStruct>
    <!-- END HIGH LEVEL SEMANTIC TYPES -->


    <!-- A Debug Message -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="DebugMessage"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="DebugMessage">
      <Attributes>
        <NativeType Name="Message" ImplementingType="string"/>
      </Attributes>
    </SemanticTypeStruct>

    <!-- A System Message -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="SystemMessage"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="SystemMessage">
      <Attributes>
        <NativeType Name="Action" ImplementingType="string"/>
        <NativeType Name="Data" ImplementingType="string"/>
        <NativeType Name="Source" ImplementingType="IReceptorInstance"/>
      </Attributes>
    </SemanticTypeStruct>

    <!-- An Animation Message -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="CarrierAnimation"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="CarrierAnimation">
      <Attributes>
        <NativeType Name="Process" ImplementingType="Action"/>
        <NativeType Name="From" ImplementingType="IReceptorInstance"/>
        <NativeType Name="To" ImplementingType="IReceptorInstance"/>
        <NativeType Name="Carrier" ImplementingType="ICarrier"/>
      </Attributes>
    </SemanticTypeStruct>

    <!-- Text to Speech -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="TextToSpeech"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="TextToSpeech">
      <Attributes>
        <NativeType Name="Text" ImplementingType="string"/>
      </Attributes>
    </SemanticTypeStruct>

    <!-- Thumbnail Image-->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="ThumbnailImage"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="ThumbnailImage">
      <Attributes>
        <SemanticElement Name="ImageFilename"/>
        <NativeType Name="Image" ImplementingType="Image"/>
      </Attributes>
    </SemanticTypeStruct>

    <!-- System Show Image-->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="SystemShowImage"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="SystemShowImage">
      <Attributes>
        <NativeType Name="From" ImplementingType="IReceptorInstance"/>
        <NativeType Name="Image" ImplementingType="Image"/>
      </Attributes>
    </SemanticTypeStruct>

    <!-- Zipcode -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="Zipcode"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="Zipcode">
      <Attributes>
        <NativeType Name="Value" ImplementingType="string"/>
      </Attributes>
    </SemanticTypeStruct>

    <!-- Location -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="Location"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="Location">
      <Attributes>
        <NativeType Name="Zipcode" ImplementingType="string"/>
        <NativeType Name="City" ImplementingType="string"/>
        <NativeType Name="StateAbbr" ImplementingType="string"/>
        <NativeType Name="State" ImplementingType="string"/>
      </Attributes>
    </SemanticTypeStruct>

    <!-- LatLong -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="LatLong"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="LatLong">
      <Attributes>
        <NativeType Name="Zipcode" ImplementingType="string"/>
        <NativeType Name="Latitude" ImplementingType="decimal"/>
        <NativeType Name="Longitude" ImplementingType="decimal"/>
      </Attributes>
    </SemanticTypeStruct>

    <!-- Weather -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="WeatherCondition"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="WeatherCondition">
      <Attributes>
        <NativeType Name="Additive" ImplementingType="string"/>
        <NativeType Name="Coverage" ImplementingType="string"/>
        <NativeType Name="Intensity" ImplementingType="string"/>
        <NativeType Name="WeatherType" ImplementingType="string"/>
        <NativeType Name="Qualifier" ImplementingType="string"/>
      </Attributes>
    </SemanticTypeStruct>
    
    <!--  -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="WeatherHazard"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="WeatherHazard">
      <Attributes>
        <NativeType Name="Phenomena" ImplementingType="string"/>
        <NativeType Name="Significance" ImplementingType="string"/>
      </Attributes>
    </SemanticTypeStruct>

    <!--  -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="WeatherInfo"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="WeatherInfo">
      <Attributes>
        <NativeType Name="Zipcode" ImplementingType="string"/>
        <NativeType Name="Summary" ImplementingType="string"/>
        <NativeType Name="High" ImplementingType="string"/>
        <NativeType Name="Low" ImplementingType="string"/>
        <NativeType Name="Conditions" ImplementingType="List&lt;dynamic&gt;"/>
        <NativeType Name="Hazards" ImplementingType="List&lt;dynamic&gt;"/>
      </Attributes>
    </SemanticTypeStruct>

    <!--  -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="IntervalTimerConfiguration"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="IntervalTimerConfiguration">
      <Attributes>
        <NativeType Name="StartDateTime" ImplementingType="DateTime"/>
        <NativeType Name="Interval" ImplementingType="int"/>
        <NativeType Name="EventName" ImplementingType="string"/>
        <NativeType Name="IgnoreMissedIntervals" ImplementingType="bool"/>
      </Attributes>
    </SemanticTypeStruct>

    <!--  -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="TimerEvent"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="TimerEvent">
      <Attributes>
        <NativeType Name="EventName" ImplementingType="string"/>
        <NativeType Name="EventDateTime" ImplementingType="DateTime"/>
      </Attributes>
    </SemanticTypeStruct>

    <!--  -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="DatabaseRecord"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="DatabaseRecord">
      <Attributes>
        <NativeType Name="ResponseProtocol" ImplementingType="string"/>
        <NativeType Name="TableName" ImplementingType="string"/>
        <NativeType Name="Action" ImplementingType="string"/>                <!-- insert, update, delete, select -->
        <NativeType Name="Row" ImplementingType="ICarrier"/>                 <!-- the carrier with the actual row data -->
        <NativeType Name="Where" ImplementingType="string"/>
        <NativeType Name="OrderBy" ImplementingType="string"/>
        <NativeType Name="GroupBy" ImplementingType="string"/>
        <NativeType Name="Tag" ImplementingType="string"/>
      </Attributes>
    </SemanticTypeStruct>

    <!--  -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="RequireTable"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="RequireTable">
      <Attributes>
        <NativeType Name="TableName" ImplementingType="string"/>
        <NativeType Name="Schema" ImplementingType="string"/>
      </Attributes>
    </SemanticTypeStruct>

    <!-- ScrapeWebpage -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="ScrapeWebpage"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="ScrapeWebpage">
      <Attributes>
        <NativeType Name="URL" ImplementingType="string"/>
        <NativeType Name="ResponseProtocol" ImplementingType="string"/>
      </Attributes>
    </SemanticTypeStruct>
    
    <!-- -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="APODWebpage"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="APODWebpage">
      <Attributes>
        <NativeType Name="URL" ImplementingType="string"/>
        <NativeType Name="HTML" ImplementingType="string"/>
        <NativeType Name="Errors" ImplementingType="string"/>
      </Attributes>
    </SemanticTypeStruct>

    <!-- Schema Collection Helpers -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="LastEventDateTimeRecordset"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="LastEventDateTimeRecordset">
      <Attributes>
        <NativeType Name="Recordset" ImplementingType="List&lt;dynamic&gt;"/>
        <NativeType Name="Tag" ImplementingType="string"/>
      </Attributes>
    </SemanticTypeStruct>

    <!-- Table schemas -->
    <!-- **** LastEventDateTIme **** -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="LastEventDateTime"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="LastEventDateTime">
      <Attributes>
        <NativeType Name="ID" ImplementingType="long?"/>
        <NativeType Name="EventName" ImplementingType="string"/>
        <NativeType Name="EventDateTime" ImplementingType="DateTime?"/>
      </Attributes>
    </SemanticTypeStruct>
    
    <!-- -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="APODRecordset"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="APODRecordset">
      <Attributes>
        <NativeType Name="Recordset" ImplementingType="List&lt;dynamic&gt;"/>
        <NativeType Name="Tag" ImplementingType="string"/>
      </Attributes>
    </SemanticTypeStruct>
    
    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="APOD"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="APOD">
      <Attributes>
        <SementicElement Name="PrimaryKey"/>
        <SemanticElement Name="URL"/>
        <SemanticElement Name="ImageFilename"/>
        <SemanticElement Name="Keywords"/>
        <SemanticElement Name="Title"/>
        <SemanticElement Name="Explanation"/>
        <NativeType Name="Errors" ImplementingType="string"/>
      </Attributes>
    </SemanticTypeStruct>

    <!-- END SCHEMA -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="GetImageMetadata"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="GetImageMetadata">
      <Attributes>
        <SemanticElement Name="ImageFilename"/>
        <NativeType Name="ResponseProtocol" ImplementingType="string"/>
      </Attributes>
    </SemanticTypeStruct>

    <!-- ==== -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="HaveImageMetadata"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="HaveImageMetadata">
      <Attributes>
        <SemanticElement Name="ImageFilename"/>
        <NativeType Name="Metadata" ImplementingType="ICarrier"/>
      </Attributes>
    </SemanticTypeStruct>

    <!-- ==== -->

    <SemanticTypeDecl OfType="Noun">
      <AttributeValues>
        <Attribute Name="Name" Value="ViewImage"/>
      </AttributeValues>
    </SemanticTypeDecl>

    <SemanticTypeStruct DeclType="ViewImage">
      <Attributes>
        <SemanticElement Name="ImageFilename"/>
      </Attributes>
    </SemanticTypeStruct>

    <!-- ==== -->
  </Collection>
</SemanticTypes>
